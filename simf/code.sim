variable {
  STG str
  STG name
  INT num1
  INT age
  STG add
  BLN alive
  INT num2
  INT num3
  BLN bol1
  BLN bol2
}

code {
  PUT $The result is: $ IN str
  ASK name
  ASK age
  ASK add
  ASK alive
  PUT OHR true false IN bol1
  PUT false IN bol2
  PUT ADD 1 2 IN num1
  PUT 100 IN num2
  PRT $Your name is $
  PRT name
  PRT OHR true AND bol1 bol2
  PUT MUL 10 ADD num1 num2 IN num3
  PRT num3
  PRT DIV MUL 10 ADD num1 num2 MUL 10 ADD num1 num2
  PRT $Goodbye!$
}
